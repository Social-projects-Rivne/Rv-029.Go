CREATE TABLE if NOT EXISTS task_manager.issues (
    id uuid,
    board_id uuid,
    board_name text,
    created_at timestamp,
    description text,
    estimate int,
    name text,
    project_id uuid,
    project_name text,
    parent uuid,
    sprint_id uuid,
    status text,
    updated_at timestamp,
    user_first_name text,
    user_id uuid,
    user_last_name text,
    PRIMARY KEY (id, board_id, sprint_id, project_id));